[
    {
        "name": "Alphabetical App Grid",
        "description": "Automatically alphabetizes applications in the app picker",
        "documentation_link": "[View on Gnome Extensions](https://extensions.gnome.org/extension/4269/alphabetical-app-grid)",
        "default_state": true,
        "category": "gnome_extension",
        "actions": [
            {
                "operation_type": "gnome_extension_install",
                "operation_args": [
                    "AlphabeticalAppGrid@stuarthayhurst"
                ]
            },
            {
                "operation_type": "gnome_extension_enable",
                "operation_args": [
                    "AlphabeticalAppGrid@stuarthayhurst"
                ]
            },
            {
                "operation_type": "dconf_value",
                "operation_args": [
                    {
                        "key": "/org/gnome/shell/extensions/alphabetical-app-grid/folder-order-position",
                        "value": "'start'"
                    }
                ]
            }
        ]
    },
