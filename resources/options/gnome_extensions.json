    {
        "name": "Background Logo",
        "description": "Adds a Fedora logo watermark to the wallpaper",
        "documentation_link": "[View on Fedora Packages](https://packages.fedoraproject.org/pkgs/gnome-shell-extension-background-logo/gnome-shell-extension-background-logo)",
        "default_state": true,
        "category": "gnome_extension",
        "actions": [
            {
                "operation_type": "package_install",
                "operation_args": [
                    "gnome-shell-extension-background-logo"
                ]
            },
            {
                "operation_type": "gnome_extension_enable",
                "operation_args": [
                    "background-logo@fedorahosted.org"
                ]
            },
            {
                "operation_type": "gsettings_value",
                "operation_args": [
                    {
                        "schema": "org.fedorahosted.background-logo-extension",
                        "key": "logo-always-visible",
                        "value": "true"
                    },
                    {
                        "schema": "org.fedorahosted.background-logo-extension",
                        "key": "logo-border",
                        "value": "25"
                    },
                    {
                        "schema": "org.fedorahosted.background-logo-extension",
                        "key": "logo-opacity",
                        "value": "20"
                    },
                    {
                        "schema": "org.fedorahosted.background-logo-extension",
                        "key": "logo-position",
                        "value": "'bottom-right'"
                    },
                    {
                        "schema": "org.fedorahosted.background-logo-extension",
                        "key": "logo-size",
                        "value": "5.0"
                    }
                ]
            }
        ]
    },
